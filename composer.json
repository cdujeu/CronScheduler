{
    "name": "ucsdmath/cron-scheduler",
    "type": "library",
    "description": "A UCSDMath PHP simple Cron Scheduler Management Component",
    "keywords": ["cron-scheduler", "ucsdmath", "cron", "auto-scheduler", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2016-03-01",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "symfony/yaml": "3.1.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.5",
        "phpunit/phpunit": "5.4.3",
        "ucsdmath/testing": "1.7.x-dev",
        "squizlabs/php_codesniffer": "2.6.1"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\CronScheduler\\": "src/CronScheduler/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7-dev"
        }
    },
    "minimum-stability": "dev"
}
